<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinevotingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineVotingSystem">
        <activity android:name=".VoterVotingActivity"></activity>
        <activity android:name=".VoterOtpActivity" />
        <activity android:name=".CCandidateActivity" />
        <activity
            android:name=".CResultsActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".CVoterActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".AdminloginActivity"
            android:parentActivityName=".AdminActivity" />
        <activity
            android:name=".ResultActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VoterActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdminActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>
